<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>AgregarReservaServlet</servlet-name>
        <servlet-class>com.lavadero.serverlet.AgregarReservaServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AgregarReservaServlet</servlet-name>
        <url-pattern>/api/reserva</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MostrarReservasServlet</servlet-name>
        <servlet-class>com.lavadero.serverlet.MostrarReservasServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MostrarReservasServlet</servlet-name>
        <url-pattern>/api/mostrarReservas</url-pattern>
    </servlet-mapping>
</web-app>
